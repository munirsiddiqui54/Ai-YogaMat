{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es5",                    // Adjust target as needed
    "module": "esnext",                 // or 'commonjs'
    "jsx": "react-jsx",  
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
